{   
    "seed": 42,
    "use_weights": true,
    "tokenizer_save_path" : "tokenizer/tokenizer-32k-total",
    "tokenizer_data_path": "/data/train/*",
    "teacher_path" : "biobert/4-epoch",
    "student_path": "medicalai/ClinicalBERT",
    "train_path": "data/pretrain_data_{}.pk",
    "valid_path": "data/valid_data.pk",
    "log_step": 100,
    "lr": 5e-5,
    "betas": [0.9, 0.999],
    "eps": 1e-6,
    "weight_decay": 0.01,
    "temperature" : 2.0,
    "exclude_from_weight_decay": ["LayerNorm", "layer_norm", "bias"],
    "num_warmup_steps": 10000,
    "max_norm": 1.0,
    "epochs": 40,
    "vocab_size": 32000,
    "batch_size": 64,
    "limit_alphabet" : 10000,
    "min_frequency" : 5,
    "save_total_limit" : 15,
    "save_strategy" : "epoch",
    "save_step" : 500,
    "alpha_ce" : 0.5,
    "gradient_accumulation_steps" : 1.0,
    "fp16" : false,
    "fp16_opt_level" : "01"
}